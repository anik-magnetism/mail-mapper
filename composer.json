{
  "name": "anikninja/mail-mapper",
  "description": "Dynamic email mapping and notification package for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    { "name": "Anik", "email": "anik89bd@gmail.com" }
  ],
  "require": {
    "php": ">=8.2",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "AnikNinja\\\\MailMapper\\\\": "src/"
    },
    "files": [
      "src/Helpers/mail_mapping_helper.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "AnikNinja\\\\MailMapper\\\\MailMapperServiceProvider"
      ]
    }
  },
  "keywords": ["laravel", "mail", "email", "notifications", "mail-mapping"],
  "homepage": "https://github.com/anikninja/mail-mapper",
  "support": {
    "issues": "https://github.com/anikninja/mail-mapper/issues",
    "source": "https://github.com/anikninja/mail-mapper"
  }
}
